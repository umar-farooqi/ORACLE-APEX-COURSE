-- TABLE CREATE --
Create Table Employee_REGISTRATIONS
(
Employee_ID Number ,
EMP_NAME Varchar2(100),
job varchar2(100),
salary NUMBER,
city varchar2(100),
D_O_B date
);

-- INSERT OPTION 
     insert into Employee_REGISTRATIONS (EMPLOYEE_ID , EMP_NAME,           JOB  ,          SALARY , CITY)
                                 VALUES (2 ,'ALI', 'SOFTWARE ENG', 60000,   'MULTAN');

--UPDATE OPTION 
UPDATE Employee_REGISTRATIONS
SET JOB = 'JUNIOR DEV'
WHERE EMPLOYEE_ID = 2

-- DELETE OPTION ---

      DELETE FROM Employee_REGISTRATIONS

-- DATA FATCH        

 SELECT * FROM Employee_REGISTRATIONS

-- PRIMARY KEY 
alter table EMPLOYEE_REGISTRATIONS Add constraint
ORACLEAPEX primary key ( "EMPLOYEE_ID" );
